  height: -webkit-calc(100% - 225px);
  height: -moz-calc(100% - 225px);
  height: calc(100% - 225px);
  }

  .faqshow, .w600 {
  height: -moz-calc(100% - 85px);
  height: -webkit-calc(100% - 85px);
  height: calc(100% - 85px);
  }
} 


@media only screen and (min-width : 600px) {
  
  .contacto {
  height: -webkit-calc(100% - 197px);
  height: -moz-calc(100% - 197px);
  height: calc(100% - 197px);
  right:50px;
  }

  .forma input[type="text"],
  .forma input[type="email"],
  .forma input[type="tel"] ,
  .forma input[type="password"]  {
    width: 400px;
  }

  .forma textarea {
    width: 600px;
  }

  .faqshow {
  width: 80%;
  margin-right: 50px;
  height: -moz-calc(100% - 30px);
  height: -webkit-calc(100% - 30px);
  height: calc(100% - 30px);
}

  .w600 {
  margin-right: 50px;
  height: -moz-calc(100% - 30px);
  height: -webkit-calc(100% - 30px);
  height: calc(100% - 30px);
}

  .formalogin {
  width: 350px;
  margin-right: 130px;
}

  .formalogin .inputforma {
    width: 340px;
}

  .formasignin {
    width: 420px;
    margin-right: 30px;
    background-position: 387px top;
}

.grupogen, .sug-group-btn {
width: -moz-calc(50% - 38px);
width: -webkit-calc(50% - 38px);
width: calc(50% - 38px);
}

}

@media only screen and (min-width : 845px) {
  
.contacto {
  width: 700px;
  height: -webkit-calc(100% - 170px);
  height: -moz-calc(100% - 170px);
  height: calc(100% - 170px);
  top: 86px;

}

.flleft {
  height: 21px;
}

}

@media only screen and (min-width : 1024px) {
  
.grupogen,  .sug-group-btn {
max-width: 250px;
}

}




	color: #e48413 !important;

}

.feedtimep {
	font-size: 10px;
	color: #878787;
	line-height: 0;
	margin-bottom: 5px;
}

.feedtimep2 {
	font-size: 10px;
	color: #878787;
	line-height: 0;
	margin-bottom: 15px;
}

.lh1 {
  line-height: 1;
  margin: 10px 0 0;
}

.feedcontentp {
	font-size: 11px;
	color: #3D3D3D;
	line-height: 1;
	padding-left: 35px;
	border-bottom: 1px solid #C9C9C9;
	padding-bottom: 5px;
	margin-bottom: 5px;
	margin-top: 7px;
}

.feedcontentp2 {
	font-size: 11px;
	color: #3D3D3D;
	line-height: 1;
	padding-left: 35px;
	border-bottom: 1px solid #C9C9C9;
	padding-bottom: 5px;
}

.minavatar2 {
	float: left;
	margin-right: 5px;
	vertical-align: middle;
}

.vermodelo {
	float:right;
	margin-left: 5px;
	margin-top: -30px;
}

.mright {
